{%- liquid
    assign show_section = false
    if section.blocks.size > 0 and section.settings.show_slideshow
        assign show_section = true
    endif
-%}
{%- if show_section -%}
	{%- liquid
        assign padding_top = section.settings.padding_top | append: 'px'
        assign padding_bottom = section.settings.padding_bottom | append: 'px'
        assign padding_top_tablet = section.settings.padding_top_tablet | append: 'px'
        assign padding_bottom_tablet = section.settings.padding_bottom_tablet | append: 'px'
        assign padding_top_mobile = section.settings.padding_top_mobile | append: 'px'
        assign padding_bottom_mobile = section.settings.padding_bottom_mobile | append: 'px'
        assign block_title = section.settings.block_title
        assign block_text = section.settings.block_text
        assign block_title_align = section.settings.block_title_align
        assign container = section.settings.container
        assign block_layout = section.settings.block_layout
        assign block_logo = section.settings.block_logo
        assign block_content_style = section.settings.block_content_style
        assign block_arrows = section.settings.block_arrows
        assign block_arrows_bg = section.settings.block_arrows_bg
        assign block_arrows_color = section.settings.block_arrows_color
        assign block_arrows_border = section.settings.block_arrows_border
        assign block_dots = section.settings.block_dots
        assign block_dots_bg = section.settings.block_dots_bg
        assign block_dots_color = section.settings.block_dots_color
        assign block_dots_border = section.settings.block_dots_border
        assign block_autoplay = section.settings.block_autoplay
        assign block_autoplay_time = section.settings.block_autoplay_time | times: 1000
        assign block_fade = section.settings.block_fade
        assign block_content = section.settings.block_content
        assign lookbook_type = section.settings.lookbook_type
        assign lookbook_style = section.settings.lookbook_style
        assign has_script = false
        if section.blocks.size > 1
            assign has_script = true
        else
            assign block_video = section.blocks | where: "type", "video"
            if block_video.size > 0
                assign has_script = true
            endif
        endif
        if block_layout == '6' or block_layout == '7'
            assign block_arrows = false
        endif
    -%}
    <style type="text/css" media="screen">
        #halo-slideshow-{{ section.id }}{
            padding-top: {{ padding_top }};
            padding-bottom: {{ padding_bottom }};
        }

        {% if block_logo != blank %}
            #halo-slideshow-{{ section.id }} .halo-block-content{
                position: relative;
            }

            #halo-slideshow-{{ section.id }} .heroCarousel-logo{
                max-width: {{ block_logo.width | append: 'px' }};
            }
        {% endif %}

        #halo-slideshow-{{ section.id }} .slick-dots li button{
            background: {{ block_dots_bg }};
            border-color: {{ block_dots_border }};
            color: {{ block_dots_color }};
        }

        #halo-slideshow-{{ section.id }} .slick-dots li button:before{
            background-color: {{ block_dots_color }};
        }

        #halo-slideshow-{{ section.id }} .slick-arrow{
            background: {{ block_arrows_bg }};
            border-color: {{ block_arrows_border }}
        }

        #halo-slideshow-{{ section.id }} .slick-arrow svg{
            fill: {{ block_arrows_color }};
            stroke: transparent;
        }

        @media (max-width: 1024px) {
            #halo-slideshow-{{ section.id }}{
                padding-top: {{ padding_top_tablet }};
                padding-bottom: {{ padding_bottom_tablet }};
            }
        }

        @media (max-width: 550px) {
            #halo-slideshow-{{ section.id }}{
                padding-top: {{ padding_top_mobile }};
                padding-bottom: {{ padding_bottom_mobile }};
            }

            #halo-slideshow-{{ section.id }} .heroCarousel.style-6 .slick-dots li.slick-active button,
            #halo-slideshow-{{ section.id }} .heroCarousel.style-7 .slick-dots li.slick-active button{
                border-bottom-color: {{ block_dots_color }} !important;
            }
        }

        {% if block_logo != blank %}
            @media (min-width: 1025px){
                #halo-slideshow-{{ section.id }} .halo-block-content{
                    padding-top: 50px;
                }
            }

            @media (min-width: 1400px){
                #halo-slideshow-{{ section.id }} .halo-block-content{
                    padding-top: 75px;
                }
            }

            @media (min-width: 1600px){
                #halo-slideshow-{{ section.id }} .halo-block-content{
                    padding-top: 100px;
                }
            }
        {% endif %}
    </style>
    <div class="halo-block halo-banner slideshow" id="halo-slideshow-{{ section.id }}" data-slideshow-block {% if has_script %} data-loader-script="{{ 'halo.slide-slideshow.js' | asset_url }}"{% endif %}>
    	{%- if container != 'full' -%}
	    	<div class="container container-{{ container }}">
	    {%- endif -%}
            {%- if block_title != blank -%}
                <div class="halo-block-header">
                    {%- if container == 'full' -%}
                        <div class="container container-{{ container }}">
                    {%- endif -%}
                        <h3 class="title text-{{ block_title_align }}">
                            <span class="text">
                                {{ block_title | escape }}
                            </span>
                        </h3>
                        {%- if block_text != blank -%}
                            <span class="desc text-{{ block_title_align }}">
                                {{ block_text }}
                            </span>
                        {%- endif -%}
                    {%- if container == 'full' -%}
                        </div>
                    {%- endif -%}
                </div>
            {%- endif -%}
	    	<div class="halo-block-content{% if lookbook_type == 'popup_card' %} halo-lookbook-box{% endif %}">
                {%- if block_logo != blank -%}
                    <div class="heroCarousel-logo img-box">
                        <div class="image image-adapt" style="padding-bottom: {{ 1 | divided_by: block_logo.aspect_ratio | times: 100 | append: '%' }}">
                            <img
                                srcset="{{ block_logo | img_url: 'master' }} 1x, {{ block_logo | img_url: 'master', scale: 2 }} 2x"
                                src="{{ block_logo | img_url: 'master' }}"
                                loading="lazy"
                                alt="{{ block_logo.alt | default: shop.name | escape }}"
                                width="{{ block_logo.width | replace: 'px', '' }}"
                                height="{{ block_logo.height | replace: 'px', '' }}"
                            >
                        </div>
                    </div>
                {%- endif -%}
	    		<div class="heroCarousel-wrapper clearfix">
	    			<div class="heroCarousel{% if block_arrows %} heroCarousel-arrows{% endif %} style-{{ block_layout }} halo-banner-wrapper"
	    				data-item-dots="{{ block_dots }}"
                        data-item-arrows="{{ block_arrows }}"
                        data-item-autoplay="{{ block_autoplay }}"
                        data-item-time="{{ block_autoplay_time }}"
                        data-item-fade="{{ block_fade }}"
                    >
                    	{%- for block in section.blocks -%}
                    		{%- liquid
                    			assign show_video = false
                    			assign has_video = false
                    			if block.type != 'image'
                    				assign show_video = true
                    				assign video_type = block.settings.video_type
                    				assign video_youtube = block.settings.video_youtube
	                                assign video_mp4 = block.settings.video_mp4
	                                assign video_vimeo = block.settings.video_vimeo
	                                assign video_height = block.settings.video_height | append: '%'
	                                if video_mp4 != blank or video_youtube != blank or video_vimeo != blank
                                        assign has_video = true
                                    endif
                    			else
                    				assign lookbook = block.settings.lookbook
                    			endif
                                assign image = block.settings.image
                                assign mobile_image = block.settings.mobile_image
                                assign title = block.settings.title
                                assign subtitle = block.settings.subtitle
                                assign text = block.settings.text
                                assign btn = block.settings.btn
                                assign btn_text = block.settings.btn_text
                                assign link = block.settings.link
                                assign position = block.settings.position
                                assign alignment = block.settings.alignment
                                assign show_slideshow_style = block.settings.show_slideshow_style
						        if show_slideshow_style
                                    assign show_background_overlay = block.settings.show_background_overlay
                                    if show_background_overlay
                                        assign background_overlay = block.settings.background_overlay
                                    endif
						            assign background_color = block.settings.background_color
						            assign title_color = block.settings.title_color
						            assign text_color = block.settings.text_color
						            assign transparent_button = block.settings.transparent_button
						            assign title_font = block.settings.title_font
						            assign title_font_size = block.settings.title_font_size
						            assign title_mobile_font_size = block.settings.title_mobile_font_size | append: 'px'
						            assign title_font_weight = block.settings.title_font_weight
						            assign title_text_transform = block.settings.title_text_transform
						            assign text_font_size = block.settings.text_font_size | append: 'px'
						            assign text_font_weight = block.settings.text_font_weight
						            assign text_text_transform = block.settings.text_text_transform
						        endif
                                if block_layout == '6'
                                    assign position = 'center'
                                elsif block_layout == '7'
                                    assign position = 'left'
                                endif
                            -%}

                            <div class="item item-{{ block.id }}" data-slide-index="{{ forloop.index0 }}">
                            	{%- if show_slideshow_style -%}
		                            <style type="text/css" media="screen">
		                            	:root {
							                {% if title_font == 'font_1' %}
							                    --title-font-{{ block.id }}: var(--font-1-family);
							                {% elsif title_font == 'font_2' %}
							                    --title-font-{{ block.id }}: var(--font-2-family);
							                {% elsif title_font == 'font_3' %}
							                    --title-font-{{ block.id }}: var(--font-3-family);
							                {% endif %}
							            }

                                        {% if show_background_overlay %}
                                            #halo-slideshow-{{ section.id }} .item-{{ block.id }} .heroCarousel-overlay-wrapper .image:before{
                                                background-color: {{ background_overlay | color_modify: 'alpha', 0.3 }};
                                            }
                                        {% endif %}

		                                {% if background_color != 'rgba(0,0,0,0)' and background_color != blank %}
		                                    #halo-slideshow-{{ section.id }} .item-{{ block.id }} .content-box{
		                                        background: {{ background_color }};
		                                    }
		                                {% endif %}

		                                #halo-slideshow-{{ section.id }} .item-{{ block.id }} .banner-title{
		                                	font-family: var(--title-font-{{ block.id }});
							                font-size: {{ title_mobile_font_size }};
							                font-weight: {{ title_font_weight }};
							                text-transform: {{ title_text_transform }};
		                                    color: {{ title_color }};
		                                }

                                        #halo-slideshow-{{ section.id }} .item-{{ block.id }} .banner-subtitle{
                                            font-size: {{ text_font_size }};
                                            font-weight: bold;
                                            text-transform: {{ title_text_transform }};
                                            color: {{ text_color }};
                                        }

		                                #halo-slideshow-{{ section.id }} .item-{{ block.id }} .banner-text{
							                font-size: {{ text_font_size }};
							                font-weight: {{ text_font_weight }};
							                text-transform: {{ text_text_transform }};
		                                    color: {{ text_color }};
		                                }

		                                {% if transparent_button %}
		                                    #halo-slideshow-{{ section.id }} .item-{{ block.id }} .banner-button{
		                                        color: {{ text_color }};
		                                        border-color: {{ text_color }};
		                                        background-color: transparent;
		                                    }

                                            #halo-slideshow-{{ section.id }} .item-{{ block.id }} .banner-button:before{
                                                background-color: {{ text_color }};
                                            }
		                                {% endif %}

		                                {% if background_color != 'rgba(0,0,0,0)' and background_color != blank %}
		                                    @media (max-width: 550px){
		                                        #halo-slideshow-{{ section.id }} .item-{{ block.id }} .content-box--absolute{
		                                            left: 50%;
		                                            transform: translate(-50%,-50%);
		                                            max-width: calc(100% - 30px);
		                                        }
		                                    }

		                                    @media (min-width: 551px){
		                                        #halo-slideshow-{{ section.id }} .item-{{ block.id }} .content-box--absolute{
		                                            padding-left: 15px;
		                                            padding-right: 15px;
		                                        }
		                                    }
		                                {% endif %}

                                        {% if title_font_size >= 60 %}
                                            @media (min-width: 768px){
                                                #halo-slideshow-{{ section.id }} .item-{{ block.id }} .banner-title{
                                                    font-size: {{ title_font_size | minus: 30 | append: 'px' }};
                                                }
                                            }

                                            @media (min-width: 1025px){
                                                #halo-slideshow-{{ section.id }} .item-{{ block.id }} .banner-title{
                                                    font-size: {{ title_font_size | minus: 20 | append: 'px' }};
                                                }
                                            }

                                            @media (min-width: 1600px){
                                                #halo-slideshow-{{ section.id }} .item-{{ block.id }} .banner-title{
                                                    font-size: {{ title_font_size | append: 'px' }};
                                                }
                                            }
                                        {% else %}
                                            @media (min-width: 551px){
                                                #halo-slideshow-{{ section.id }} .item-{{ block.id }} .banner-title{
                                                    font-size: {{ title_font_size | append: 'px' }};
                                                }
                                            }
                                        {% endif %}
		                            </style>
		                        {%- endif -%}
                            	<div class="heroCarousel-slide{% if forloop.first == true %} heroCarousel-slide--first{% endif %} banner-item" data-position="{{ position }}">
                            		<div class="heroCarousel-slide--wrapper">
                            			{%- if show_video and has_video -%}
                            				<div class="heroCarousel-box">
                            			{%- endif -%}
                                        {%- if show_background_overlay -%}
                                            <div class="heroCarousel-image-wrapper heroCarousel-overlay-wrapper">
                                        {%- endif -%}
                            				<div class="heroCarousel-image img-box img-box--mobile">
                            					{%- if image != blank -%}
				                                    <{% if link != blank %}a{% else %}div{% endif %} class="image image-adapt" {% if link != blank %} href="{{ link }}"{% endif %} style="padding-top: {{ 1 | divided_by: image.aspect_ratio | times: 100 | append: '%' }};">
				                                        <img srcset="{{ image | img_url: '1920x' }} 1x, {{ image | img_url: '1920x', scale: 2 }} 2x" src="{{ image | img_url: '1920x', crop: 'center' }}" alt="{{ image.alt | default: shop.name | escape }}" loading="lazy">
                                                        {% unless show_background_overlay %}
                                                        {% endunless %}
				                                    </{% if link != blank %}a{% else %}div{% endif %}>
				                                {%- else -%}
				                                    <{% if link != blank %}a{% else %}div{% endif %} class="image" {% if link != blank %} href="{{ link }}"{% endif %}>
				                                        {% capture current %}{% cycle 1, 2 %}{% endcapture %}
				                                        {{ 'lifestyle-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}   
				                                    </{% if link != blank %}a{% else %}div{% endif %}>
				                                {%- endif -%}
				                                {%- if mobile_image != blank -%}
				                                    <{% if link != blank %}a{% else %}div{% endif %} class="image image-mobile image-adapt" {% if link != blank %} href="{{ link }}"{% endif %} style="padding-top: {{ 1 | divided_by: mobile_image.aspect_ratio | times: 100 | append: '%' }}">
				                                        <img srcset="{{ mobile_image | img_url: 'master' }} 1x, {{ mobile_image | img_url: 'master', scale: 2 }} 2x" src="{{ mobile_image | img_url: 'master' }}" alt="{{ mobile_image.alt | default: shop.name | escape }}" loading="lazy">
				                                    </{% if link != blank %}a{% else %}div{% endif %}>
				                                {%- else -%}
				                                    {%- if image != blank -%}
				                                        <{% if link != blank %}a{% else %}div{% endif %} class="image image-mobile image-adapt" {% if link != blank %} href="{{ link }}"{% endif %} style="padding-top: 100%">
				                                            <img srcset="{{ image | img_url: '420x', crop: 'center' }} 1x, {{ image | img_url: '420x', crop: 'center',  scale: 2 }} 2x" src="{{ image | img_url: '420x', crop: 'center' }}" alt="{{ image.alt | default: shop.name | escape }}" loading="lazy">
				                                        </{% if link != blank %}a{% else %}div{% endif %}>
				                                    {%- else -%}
				                                        <{% if link != blank %}a{% else %}div{% endif %} class="image image-mobile" {% if link != blank %} href="{{ link }}"{% endif %}>
				                                            {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
				                                        </{% if link != blank %}a{% else %}div{% endif %}>
				                                    {%- endif -%}
				                                {%- endif -%}
                            				</div>
                                        {%- if show_background_overlay -%}
                                            </div>
                                        {%- endif -%}
                        				{%- if show_video and has_video -%}
                                                <div class="heroCarousel-video video-box" style="padding-bottom: {{ video_height }};">
                                                    {%- if video_type == 'youtube' -%}
                                                        <div lass="video youtube" data-youtube-video="{{ video_youtube }}"></div>
                                                    {%- elsif video_type == 'mp4' -%}
                                                        <div lass="video mp4" data-mp4-video>
                                                            <video loop autoplay="autoplay">
                                                                <source type="video/mp4" src="{{ video_mp4 }}">
                                                            </video>
                                                        </div>
                                                    {%- elsif video_type == 'vimeo' -%}
                                                        <div class="video vimeo" data-vimeo-video>
                                                            <iframe src="{{ video_vimeo }}" allowfullscreen frameborder="0" mozallowfullscreen webkitallowfullscreen></iframe>
                                                        </div>
                                                    {%- endif -%}
                                                </div>
                            				</div>
                            			{%- endif -%}
                            			{%- if subtitle != blank or title != blank or text != blank or btn_text != blank -%}
                            				<div class="heroCarousel-content content-box content-box--absolute content-box--full content-box--{{ position }} text-{{ alignment }}{% if block_content == 'relative' %} content-box--relative{% endif%} style-{{ block_content_style }}">
                                                {%- if subtitle != blank -%}
                                                    <span class="heroCarousel-subtitle banner-subtitle animated">
                                                        {{ subtitle | escape }}
                                                    </span>
                                                {%- endif -%}
                            					{%- if title != blank -%}
			                                        <h4 class="heroCarousel-title banner-title animated">
			                                            {{ title | escape }}
			                                        </h4>
			                                    {%- endif -%}
			                                    {%- if text != blank -%}
			                                        <span class="heroCarousel-description banner-text animated">
			                                            {{ text }}
			                                        </span>
			                                    {%- endif -%}
			                                    {%- if btn_text != blank -%}
			                                    	<{% if link != blank %}a{% else %}div{% endif %} {% if link != blank %} href="{{ link }}"{% endif %} class="heroCarousel-action banner-button button button-{{ btn }}{% if transparent_button %} button--transparent{% endif %} animated">
                                                        {{ btn_text | escape }}
                                                    </{% if link != blank %}a{% else %}div{% endif %}>
			                                    {%- endif -%}
                            				</div>
                            			{%- endif -%}
                            			{%- if lookbook -%}
                                            <div class="heroCarousel-lookbook">
                                                {%- for i in (1..5) -%}
                                                    {% capture cap_product %}product_{{ i }}{% endcapture %}
                                                    {% capture cap_point_y %}point_y_{{ i }}{% endcapture %}
                                                    {% capture cap_point_x %}point_x_{{ i }}{% endcapture %}
                                                    {%- liquid
                                                    	assign handle = block.settings[cap_product]
                                                    	assign product = all_products[handle]
                                                    	assign point_y = block.settings[cap_point_y]
                                                    	assign point_x = block.settings[cap_point_x]
                                                    -%}
                                                    {%- if product != blank -%}
                                                        <div class="point-box style-{{ lookbook_style }}" style="top: {{ point_y }}%; left: {{ point_x }}%;">
                                                            <div class="point-icon" {% if lookbook_type == 'popup_quickview' %} data-open-quick-view-popup{% else %} data-open-lookbook-popup{% endif %} data-product-handle="{{ product.handle }}" role="button" tabindex="0">
                                                                <span class="text{% if lookbook_style == '3' %} visually-hidden{% endif %}">
                                                                    {%- if lookbook_style == '4' -%}
                                                                        {% render 'icon-plus' %}
                                                                    {%- else -%}
                                                                        {{ forloop.index }}
                                                                    {%- endif -%}
                                                                </span>
                                                            </div>
                                                        </div>
                                                    {%- endif -%}
                                                {%- endfor -%}
                                            </div>
                                        {%- endif -%}
                            		</div>
                            	</div>
                            </div>
                    	{%- endfor -%}
                    </div>
	    		</div>
                {%- if lookbook_type == 'popup_card' -%}
                    <div class="halo-lookbook-popup">
                        <a href="#" class="close" data-close-lookbook-popup role="button">
                            <span class="visually-hidden">{{ 'general.common.close' | t }}</span>
                            {% render 'icon-close' %}
                        </a>
                        <div class="content"></div>
                    </div>
                {%- endif -%}
	    	</div>
	    {%- if container != 'full' -%}
	    	</div>
	    {%- endif -%}
    </div>
{%- endif -%}
{% schema %}
	{
        "name": "t:sections.slideshow.name",
        "settings": [
            {
                "type": "header",
                "content": "t:sections.slideshow.settings.header__1.content"
            },
            {
                "type": "checkbox",
                "id": "show_slideshow",
                "label": "t:sections.slideshow.settings.general.label__1",
                "default": true
            },
            {
                "type": "select",
                "id": "container",
                "label": "t:sections.general.settings.container.label__1",
                "default": "full",
                "options": [
                {
                    "value": "1170",
                    "label": "t:sections.general.settings.container.options__1"
                },
                {
                    "value": "1370",
                    "label": "t:sections.general.settings.container.options__2"
                },
                {
                    "value": "1570",
                    "label": "t:sections.general.settings.container.options__3"
                },
                {
                    "value": "1770",
                    "label": "t:sections.general.settings.container.options__4"
                },
                {
                    "value": "1860",
                    "label": "t:sections.general.settings.container.options__5"
                },
                {
                    "value": "full",
                    "label": "t:sections.general.settings.container.options__6"
                }],
                "info": "t:sections.general.settings.container.info"
            },
            {
                "type": "paragraph",
                "content": "t:sections.general.settings.padding.paragraph__1.content"
            },
            {
                "type": "range",
                "id": "padding_top",
                "label": "t:sections.general.settings.padding.label__1",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 0
            },
            {
                "type": "range",
                "id": "padding_bottom",
                "label": "t:sections.general.settings.padding.label__2",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 0
            },
            {
                "type": "paragraph",
                "content": "t:sections.general.settings.padding.paragraph__2.content"
            },
            {
                "type": "range",
                "id": "padding_top_tablet",
                "label": "t:sections.general.settings.padding.label__1",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 0
            },
            {
                "type": "range",
                "id": "padding_bottom_tablet",
                "label": "t:sections.general.settings.padding.label__2",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 0
            },
            {
                "type": "paragraph",
                "content": "t:sections.general.settings.padding.paragraph__3.content"
            },
            {
                "type": "range",
                "id": "padding_top_mobile",
                "label": "t:sections.general.settings.padding.label__1",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 0
            },
            {
                "type": "range",
                "id": "padding_bottom_mobile",
                "label": "t:sections.general.settings.padding.label__2",
                "min": 0,
                "max": 100,
                "step": 1,
                "unit": "t:sections.general.settings.padding.unit",
                "default": 0
            },
            {
