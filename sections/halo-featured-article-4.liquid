{%- liquid
    assign show_section = false
    if section.blocks.size > 0 and section.settings.show_featured_article
        assign show_section = true
    endif
-%}
{%- if show_section -%}
	{%- liquid
        assign padding_top = section.settings.padding_top | append: 'px'
        assign padding_bottom = section.settings.padding_bottom | append: 'px'
        assign padding_top_tablet = section.settings.padding_top_tablet | append: 'px'
        assign padding_bottom_tablet = section.settings.padding_bottom_tablet | append: 'px'
        assign padding_top_mobile = section.settings.padding_top_mobile | append: 'px'
        assign padding_bottom_mobile = section.settings.padding_bottom_mobile | append: 'px'
        assign container = section.settings.container
        assign block_bg = section.settings.block_bg
        assign col = section.settings.col
        assign block_title = section.settings.block_title
        assign block_text = section.settings.block_text
        assign block_subtitle = section.settings.block_subtitle
        assign button_title = section.settings.button_title
        assign button_link = section.settings.button_link
        assign btn_header = section.settings.btn_header
        assign block_title_align = section.settings.block_title_align
        assign media_size = section.settings.image_ratio
        assign portrait_aspect_ratio = section.settings.portrait_aspect_ratio | append: '%'
        assign show_summary = section.settings.show_summary
        assign show_author = section.settings.show_author
        assign show_date = section.settings.show_date
        assign show_read_more = section.settings.show_read_more
        assign align = section.settings.text_align
        assign background_color = section.settings.background_color
        assign title_color = section.settings.title_color
        assign text_color = section.settings.text_color
        assign title_font = section.settings.title_font
        assign title_font_size = section.settings.title_font_size | append: 'px'
        assign title_mobile_font_size = section.settings.title_mobile_font_size | append: 'px'
        assign title_font_weight = section.settings.title_font_weight
        assign title_text_transform = section.settings.title_text_transform
        assign text_font_size = section.settings.text_font_size | append: 'px'
        assign text_font_weight = section.settings.text_font_weight
        assign text_text_transform = section.settings.text_text_transform
        assign image_border_radius = settings.image_border_radius
        assign image_effect = settings.image_effect
        assign column = '2'
        if section.blocks.size == 1
        	assign column = '1'
        endif
    -%}
    <style type="text/css" media="screen">
    	:root {
            {% if title_font == 'font_1' %}
                --title-font-{{ section.id }}: var(--font-1-family);
            {% elsif title_font == 'font_2' %}
                --title-font-{{ section.id }}: var(--font-2-family);
            {% elsif title_font == 'font_3' %}
                --title-font-{{ section.id }}: var(--font-3-family);
            {% endif %}
        }

    	#halo-featured-article-{{ section.id }}{
            padding-top: {{ padding_top }};
            padding-bottom: {{ padding_bottom }};
            {% if block_bg != 'rgba(0,0,0,0)' and block_bg != blank %}
            	background: {{ block_bg }};
            {% endif %}
        }

        {% if background_color != 'rgba(0,0,0,0)' and background_color != blank %}
        	#halo-featured-article-{{ section.id }} .article-item{
        		box-shadow: 0 1px 3px -2px rgb(0, 0, 0, .1), 0 1px 2px -2px rgb(0, 0, 0, .15);
        	}

        	#halo-featured-article-{{ section.id }} .content-box{
                background-color: {{ background_color }};
                padding-left: 15px;
                padding-right: 15px;
            }
        {% endif %}

        #halo-featured-article-{{ section.id }} .banner-title{
            font-family: var(--title-font-{{ section.id }});
            font-size: {{ title_mobile_font_size }};
            font-weight: {{ title_font_weight }};
            text-transform: {{ title_text_transform }};
            color: {{ title_color }};
        }

        #halo-featured-article-{{ section.id }} .banner-info{
        	font-size: calc({{ text_font_size }} - 2px);
            font-weight: {{ text_font_weight }};
            font-style: italic;
            text-transform: {{ text_text_transform }};
        }

        #halo-featured-article-{{ section.id }} .banner-summary{
            font-size: {{ text_font_size }};
            font-weight: {{ text_font_weight }};
            text-transform: {{ text_text_transform }};
        }

        #halo-featured-article-{{ section.id }} .banner-info,
        #halo-featured-article-{{ section.id }} .banner-summary{
            color: {{ text_color }};
        }

        @media (max-width: 1024px) {
            #halo-featured-article-{{ section.id }}{
                padding-top: {{ padding_top_tablet }};
                padding-bottom: {{ padding_bottom_tablet }};
            }
        }

        @media (max-width: 550px) {
            #halo-featured-article-{{ section.id }}{
                padding-top: {{ padding_top_mobile }};
                padding-bottom: {{ padding_bottom_mobile }};
            }
        }

        {% if background_color != 'rgba(0,0,0,0)' and background_color != blank %}
	        @media (min-width: 321px){
	        	#halo-featured-article-{{ section.id }} .content-box{
	        		padding-left: 30px;
	        		padding-right: 30px;
	        	}
	        }
	    {% endif %}

        @media (min-width: 551px){
            #halo-featured-article-{{ section.id }} .banner-title{
                font-size: {{ title_font_size }};
            }
        }

        {% if background_color != 'rgba(0,0,0,0)' and background_color != blank %}
	        @media (min-width: 1025px){
	        	#halo-featured-article-{{ section.id }} .content-box{
	        		padding-left: 40px;
	        		padding-right: 40px;
	        	}
	        }
	    {% endif %}

	    {% if background_color != 'rgba(0,0,0,0)' and background_color != blank %}
	        @media (min-width: 1600px){
	        	#halo-featured-article-{{ section.id }} .content-box{
	        		padding-left: 50px;
	        		padding-right: 50px;
	        	}
	        }
	    {% endif %}
    </style>
    <div class="halo-block halo-banner halo-article halo-article-4" id="halo-featured-article-{{ section.id }}">
    	<div class="container container-{{ container }}">
			{%- if block_title != blank -%}
				<div class="halo-items halo-items-2 clearfix">
	                <div class="halo-block-header halo-item">
	                	<div class="halo-block-headerWapper text-{{ block_title_align }}">
	                		{%- if block_subtitle != blank -%}
	                			<span class="subtitle">{{ block_subtitle | escape}}</span>
	                		{%- endif -%}
		                    <h3 class="title text-{{ block_title_align }}">
		                        <span class="text">
		                            {{ block_title | escape }}
		                        </span>
		                    </h3>
		                    {%- if block_text != blank -%}
		                        <span class="desc text-{{ block_title_align }}">
		                            {{ block_text }}
		                        </span>
		                    {%- endif -%}
		                    {%- if button_title != blank -%}
		                        <{% if button_link != blank %}a{% else %}div{% endif %} {% if button_link != blank %} href="{{ button_link }}"{% endif %} class="header-button button button-{{ btn_header }} text-{{ block_title_align }}">
		                            {{ button_title | escape }}
		                        </{% if button_link != blank %}a{% else %}div{% endif %}>
		                    {%- endif -%}
	                	</div>
	                </div>
            {%- endif -%}
    		<div class="halo-block-content{% if block_title != blank %} halo-item{% endif %}">
    			<div class="halo-row halo-row--{{ col }} halo-row--grid column-{{ column }} disable-scrollbar">
                	{%- for block in section.blocks -%}
                		{%- liquid
                            assign handle = block.settings.article
                            assign article = articles[handle]
                        -%}
                        <div class="halo-row-item item item-{{ block.id }}">
                        	<div class="article-item banner-item image-{{ image_effect }} image-border-{{ image_border_radius }} banner-item--large">
                        		{%- if article != blank -%}
                        			{%- liquid
                                        assign image = article.image
                                        assign link = article.url
                                        assign title = article.title
                                        assign author = article.author
                                        assign summary = article.excerpt_or_content | strip_html | truncatewords: 50
                                    -%}
                                    <div class="img-box">
                                    	{%- if article.image -%}
                                    		<a class="image image-{{ media_size }}" href="{{ link }}"
                                                {% if media_size == 'adapt' %}
                                                    style="padding-bottom: {{ 1 | divided_by: image.aspect_ratio | times: 100 }}%;"
                                                {% elsif media_size == 'square' %}
                                                    style="padding-bottom: 100%;"
                                                {% elsif media_size == 'portrait' %}
                                                    {% if portrait_aspect_ratio != blank %}
                                                        style="padding-bottom: {{ portrait_aspect_ratio }};"
                                                    {% endif %}
                                                {% endif %}
                                            >
                                                <img srcset="
                                                    {%- if image.width >= 165 -%}
                                                        {{ image | img_url: '165x' }} 165w,
                                                    {%- endif -%}
                                                    {%- if image.width >= 360 -%}
                                                        {{ image | img_url: '360x' }} 360w,
                                                    {%- endif -%}
                                                    {%- if image.width >= 533 -%}
                                                        {{ image | img_url: '533x' }} 533w,
                                                    {%- endif -%}
                                                    {%- if image.width >= 720 -%}
                                                        {{ image | img_url: '720x' }} 720w,
                                                    {%- endif -%}
                                                    {%- if image.width >= 940 -%}
                                                        {{ image | img_url: '940x' }} 940w,
                                                    {%- endif -%}
                                                    {%- if image.width >= 1066 -%}
                                                        {{ image | img_url: '1066x' }} 1066w
                                                    {%- endif -%}"
                                                    src="{{ image | img_url: '533x' }}"
                                                    alt="{{ image.alt | default: shop.name | escape }}"
                                                    sizes="(min-width: 1100px) 535px, (min-width: 750px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2)"
                                                    loading="lazy"
                                                >
                                            </a>
                                    	{%- else -%}
                                    		<a class="image" href="{{ link }}">
                                                {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                                            </a>
                                    	{%- endif -%}
                                    </div>
                                    <div class="content-box text-{{ align }}">
                                    	{%- if show_author or show_date -%}
                                            <span class="banner-info">
                                                {% if show_author %}{{ 'blogs.article.author' | t : author : author }}{% endif %}{% if show_date %}{{ article.published_at | time_tag: '%b %d, %Y' }}{% endif%}
                                            </span>
                                        {%- endif -%}
                                        {%- if title != blank -%}
                                            <h4 class="banner-title">
                                                <a href="{{ link }}">  
                                                    {{ title | escape }}
                                                </a>
                                            </h4>
                                        {%- endif -%}
                                        {%- if show_summary -%}
                                            <div class="banner-summary">
                                                {{ summary | escape }}
                                            </div>
                                        {%- endif -%}
                                        {%- if show_read_more -%}
                                            <a href="{{ link }}" class="banner-link link link-underline text-underline">
                                                <span class="text">{{ 'blogs.article.read_more' | t }}</span>
                                            </a>
                                        {%- endif -%}
                                    </div>
                        		{%- else -%}
                        			<div class="img-box">
                                        <div class="image">
                                            {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                                        </div>
                                    </div>
                                    <div class="content-box text-{{ align }}">
                                    	{%- if show_author or show_date -%}
                                            <span class="banner-info">
                                                {% if show_author %}Article's Author{% endif %}{% if show_author and show_date %} - {% endif %}{% if show_date %}The timestamp of when the article was published{% endif %}
                                            </span>
                                        {%- endif -%}
                                        <h4 class="banner-title">
                                            <a href="#">  
                                                Article Title
                                            </a>
                                        </h4>
                                        {%- if show_summary -%}
                                            <div class="banner-summary">
                                                Lorem ipsum dolor sit amet, consectetur adipiscing elit. Morbi tincidunt nunc at eros tempus, ac scelerisque dolor luctus.
                                            </div>
                                        {%- endif -%}
                                    </div>
                        		{%- endif -%}
                        	</div>
                        </div>
                	{%- endfor -%}
                </div>
    		</div>
    		{%- if block_title != blank -%}
    			</div>
    		{%- endif -%}
    	</div>
    </div>
{%- endif -%}
